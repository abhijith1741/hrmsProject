<body class="hold-transition sidebar-mini layout-fixed">
<div class="wrapper">

  <!-- Preloader -->

<%@ include file="header.jsp" %>

  
  
 
<%
	String username = (String) session.getAttribute("username");
	List<userDetailsBean> users = (List<userDetailsBean>) session.getAttribute("list");
		
%>	

<div class="limiter">
		<div class="container-table100">
			<div class="wrap-table100">
				<div class="table100 ver1 m-b-110">
					<div class="table100-head">
						<table>
							<thead>
								<tr class="row100 head">
									<th class="cell100 column1">Id</th>
									<th class="cell100 column2">Name</th>
									<th class="cell100 column3">Email</th>
									<th class="cell100 column4">Mobile</th>
									<th class="cell100 column5">Password</th>
								</tr>
							</thead>
						</table>
					</div>

					<div class="table100-body js-pscroll">
						<table>
							<tbody>
							
							 <%
								for(userDetailsBean user : users){

							%>   
								<tr class="row100 body">
									<td class="cell100 column1"><%=user.getId() %></td>
									<td class="cell100 column2"><%=user.getName() %></td>
									<td class="cell100 column3"><%=user.getEmail() %></td>
									<td class="cell100 column4"><%=user.getMobile() %></td>
									<td class="cell100 column5"><%=user.getPassword() %></td>
								</tr>
							<%
  								}
							%>	
							</tbody>
						</table>
					</div>
				</div>
				
			</div>
		</div>
	</div>



	
  <footer class="main-footer" style="margin-left:0; background:lightslategrey;">
    <strong style="color:white;">Copyright &copy;2021. HRMS all rights reserved..</strong>
    All rights reserved.
    <div class="float-right d-none d-sm-inline-block">
      <b>Version</b> 3.1.0
    </div>
  </footer>
    <aside class="control-sidebar control-sidebar-dark">
    <!-- Control sidebar content goes here -->
  </aside>
</div>

</body>
